<MyGrid>:

    userId: userId
    zipC: zipC
    GridLayout:
        cols:1
        size: root.width, root.height
        GridLayout:
            cols:2

            Label:
                text: "userId: "
            TextInput:
                id: userId
                multiline: False
            Label:
                text: "Zip Code: "
            TextInput:
                id: zipC
                multiline:False
        Button:
            text: "Submit"
            on_press: root.btn()

